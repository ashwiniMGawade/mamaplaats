<div class="row-fluid">
	<div class="span8">
		<h1>Testpanel</h1>
	</div>
	<div class="span4">
		<div class="pull-right">
			<a href="/admin/review-product" class="btn right">Nieuwe Testpanel</a>
		</div>
	</div>
</div>
<div class="row-fluid sortable ui-sortable">
	<div class="box span12">
		<div class="box-header" data-original-title>
			<h2><i class="icon-camera edit"></i><span class="break"></span>Testpanel Lijstweergave</h2>
			<div class="box-icon">
				<a href="#" class="btn-minimize"><i class="icon-chevron-up"></i></a>
				<a href="#" class="btn-close"><i class="icon-remove"></i></a>
			</div>
		</div>
		<div class="box-content">
			<div class="row-fluid">
				<table class="table table-striped table-bordered bootstrap-datatable datatable">
					<thead>
						<tr>
							<th>Titel</th>
							<th>Inschrijf Start</th>
							<th>Inschrijf Eind</th>
							<th>Laatst Gewijzigd</th>
							<th>Acties</th>
						</tr>
					</thead>
					<tbody>
						{{#product_list}}
						<tr>
							<td><a href="/admin/review-product/{{id}}" class="boldtext">{{title}}</a></td>
							<td>{{application_start_date}}</td>
							<td>{{application_end_date}}</td>
							<td>{{modified}}</td>
							<td>
								<a class="btn" href="/admin/review-product/{{id}}">
									<i class="halflings-icon edit"></i>
								</a>
							</td>
						</tr>
						{{/product_list}}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>