<div id="page">
	<div class="container" style="position: relative;">
        <div id="user-menu" class="navigation user-menu">
            {{> profile_rt_side_menu}}
        </div>
		<header id="header">
			<div class="row">
				<div class="main-panel">
                    <a href="/" class="opener">menu</a>
					<strong class="logo"><a href="/">MamaPlaats</a></strong>
					<div class="quick-access">
                        <div class="header-pencil item"  title="Schrijf nieuwe blog">
                            <a href="/user/blog/index"><i class="glyphicon glyphicon-pencil"></i></a>
                        </div>
						<div class="search item">
							<a href="#" class="opener">Search LInk</a>
							<div class="slide">
                                {{> global_seach_box}}
							</div>
						</div>
						<div class="web item">
							{{^logged_in}}
							<a href="/user/login" class="linker">Web Link</a>
							{{/logged_in}}
							{{#logged_in}}
							<a href="#" class="opener">Web LInk</a>
							<div class="slide">
								<strong class="ti.quick-access .searchtle">Meldingen:</strong>
								<div class="list">
									{{#timeline}}
									<article class="list-item">
										<div class="photo">
											<a href="{{path}}" class="photo-wrap"><b><img src="{{image_path}}" width="100" height="100" alt="image description"></b></a>
										</div>
										<div class="description">
											<strong class="name"><a href="{{path}}">{{content}}</a></strong>
											<span class="date">{{date}} geleden</span>
										</div>
									</article>
									{{/timeline}}
								</div>
								<div class="link-wrap">
									<a href="/user/profile" class="link-all">All meldingen bekijken</a>
								</div>
							</div>
							{{/logged_in}}
						</div>
                        <div class="login item">
                            <a href="#" class="opener {{#logged_in}}login-opener{{/logged_in}} {{^logged_in}}redirect-to-login{{/logged_in}}">User Link</a>
                        </div>
					</div>
					<nav role="navigation">
						<ul class="main-nav">
							{{#main_menu}}
							<li id="{{text}}" class="common-nav{{#active}} active{{/active}}"><a href="{{path}}">{{text}}</a></li>
							{{/main_menu}}
						</ul>
					</nav>
				</div>
			</div>
			<div class="row">
                {{#main_menu}}
                    <div class="sub-panel subnav-over common-nav" id="{{text}}">
                        <ul class="sub-nav tar">
                            {{#sub_menu}}
                                <li {{#active}}class="active"{{/active}}><a href="{{path}}" onmouseover="_gaq.push(['_trackEvent', 'submenu', 'hover', 'user hover submenu'])">{{text}}</a></li>
                            {{/sub_menu}}
                        </ul>
                        {{> global_seach_box}}
                    </div>
                {{/main_menu}}

				<div class="sub-panel" id="main-sub-menu">
					{{#main_menu}}	{{#active}}
					<nav>
						<ul class="sub-nav tar">
							{{#sub_menu}}
							<li {{#active}}class="active"{{/active}}><a href="{{path}}">{{text}}</a></li>
							{{/sub_menu}}
						</ul>
					</nav>
					{{/active}}{{/main_menu}}
                     {{> global_seach_box}}
				</div>
			</div>
		</header> <!-- end #header -->
	</div>

